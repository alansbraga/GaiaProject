@model GaiaCore.Gaia.GaiaGame

<div class="row">

    <div class="col-md-3">
        <div class="row">
            <div>
                <div class="panel panel-default">
                    <!-- Default panel contents -->
                    <div class="panel-heading">回合助推板</div>
                    <div class="panel-body"  id="rbt_s_list" >

                        @foreach (var item in Model.RBTList)
