                @foreach (var item in Model.ATTList)
                {
                    <td title="@item.name"> <div class="attcss @{@item.name}False"><span>@item.name<br />@item.desc</span></div></td>
                }
                                                                 </tr>
                                                                 <tr>
                                                                     <td >
                                                                         基础科技(6)
                                                                     </td>
