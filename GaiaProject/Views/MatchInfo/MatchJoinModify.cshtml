@model GaiaDbContext.Models.HomeViewModels.MatchJoinModel

@{
    ViewBag.Title = "编辑";
    Layout = "_Layout";
}

<form asp-controller="MatchInfo" asp-action="MatchJoinModify" asp-route-returnurl="@ViewData["ReturnUrl"]" method="post" class="form-horizontal">
    <hr />
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="username" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="username" class="form-control" />
            <span asp-validation-for="username" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="first" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="first" class="form-control" />
            <span asp-validation-for="first" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="second" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="second" class="form-control" />
            <span asp-validation-for="second" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="third" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="third" class="form-control" />
            <span asp-validation-for="third" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="fourth" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="fourth" class="form-control" />
            <span asp-validation-for="fourth" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <label asp-for="Score" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="Score" class="form-control" />
            <span asp-validation-for="Score" class="text-danger"></span>
        </div>
    </div>


    <div class="form-group">
        <label asp-for="Rank" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="Rank" class="form-control" />
            <span asp-validation-for="Rank" class="text-danger"></span>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button type="submit" class="btn btn-default">确定</button>
        </div>
    </div>
</form>

@section Scripts {
    @{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }
}